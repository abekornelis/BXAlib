.********************************************************************** 00230001
.*                                                                      00231000
.* Declare variables                                                    00240000
         GBLB  &BXA_MAC_MAPECB         * Map has been generated?        00250000
.*                                                                      00270000
.* Check parameters                                                     00280000
&LABEL   CHKMAP DSECT=&DSECT,          *                               *00290000
               CB=&CB,                 *                               *00300000
               PRFX=&PRFX              *                                00310000
         AIF   (&BXA_RC GT 4).MEND                                      00320002
.*                                                                      00330000
.* Check the CB parameter                                               00340000
         AIF   ('&DSECT' EQ 'YES').DSECT                                00350000
         AIF   ('&CB' EQ 'ECB').EMBED                                   00360000
         AIF   ('&CB' EQ 'ECBE').EMBED                                  00370000
.ERR5    MNOTE 8,'&CB control block not supported'                      00380000
         MEXIT                                                          00390000
.*                                                                      00400000
.* Generate embedded definition                                         00410000
.EMBED   ANOP                                                           00420000
&LABEL   NESTCB &CB,X,&PRFX                                             00430000
         MEXIT                                                          00440000
.*                                                                      00450000
.DSECT   ANOP                                                           00460000
         AIF   (&BXA_MAC_MAPECB).MEND                                   00470002
&BXA_MAC_MAPECB SETB 1                                                  00480000
.* Set up translation from control block to macro for DCL               00490000
         GBLC  &BXA_CB_ECB                                              00500000
         GBLC  &BXA_CB_ECBE                                             00510000
&BXA_CB_ECB SETC 'ECB'                                                  00520000
&BXA_CB_ECBE SETC 'ECB'                                                 00530000
.*                                                                      00540000
.* ECBCC - COMPLETION CODE BYTE                                         00550000
ECBCC    DSOVR X                                                        00560000
ECBWAIT  EQUOVR ,,b,ECBCC                                               00570000
ECBPOST  EQUOVR ,,b,ECBCC                                               00580000
ECBUNWT  EQUOVR ,,v,ECBCC                                               00590000
ECBNORM  EQUOVR ,,v,ECBCC                                               00600000
ECBPERR  EQUOVR ,,v,ECBCC                                               00610000
ECBDAEA  EQUOVR ,,v,ECBCC                                               00620000
ECBABEND EQUOVR ,,v,ECBCC                                               00630000
ECBINCPT EQUOVR ,,v,ECBCC                                               00640000
ECBREPRG EQUOVR ,,v,ECBCC                                               00650000
ECBEHALT EQUOVR ,,v,ECBCC                                               00660000
ECBERPAB EQUOVR ,,v,ECBCC                                               00670000
ECBERPER EQUOVR ,,v,ECBCC                                               00680000
ECBSETEO EQUOVR ,,v,ECBCC                                               00690000
ECBDMQDS EQUOVR ,,v,ECBCC                                               00700000
ECBSEQER EQUOVR ,,v,ECBCC                                               00710000
ECBINVMD EQUOVR ,,v,ECBCC                                               00720000
ECBWKOVR EQUOVR ,,v,ECBCC                                               00730000
ECBNOMSG EQUOVR ,,v,ECBCC                                               00740000
ECBDTRAQ EQUOVR ,,v,ECBCC                                               00750000
ECBEOQ   EQUOVR ,,v,ECBCC                                               00760000
ECBRAQMT EQUOVR ,,v,ECBCC                                               00770000
.*                                                                      00780000
.* ECBCCCNT - ZEROES OR REMAINDER OF COMPLETION CODE                    00790000
ECBCCCNT DSOVR 0XL3                                                     00800000
.*                                                                      00810000
.* ECBBYTE3 - THIRD BYTE OF ECBEVTBA                                    00820000
ECBEXTND EQUOVR ,,v,ECBBYTE3                                            00830000
ECBEVNT  EQUOVR ,,v,ECBBYTE3                                            00840000
.*                                                                      00850000
.* ECBEVAL - FUNCTION CODE                                              00860000
ECBEEXIT EQUOVR ,,v,ECBEVAL                                             00870000
.*                                                                      00880000
.* ECBEFLG1 - FLAG BYTE                                                 00890000
ECBEMODE EQUOVR ,,b,ECBEFLG1                                            00900000
.*                                                                      00910000
         MNOTE *,'Including IHAECB mapping macro'                       00920000
         IHAECB DSECT=YES,             * Include DSECT statement       *00930000
               EXT=YES                 *   and ECB extension            00940000
         DSOVR *END                    *                                00950000
         EQUOVR *END                   *                                00960000
.*                                                                      00970000
ECB      DSECT                                                          00980000
         ORG                                                            00990000
ECB_LEN  EQU   *-ECB                   * Length of ECB                  01000000
ECBE     DSECT                                                          01010000
         ORG                                                            01020000
ECBE_LEN EQU   *-ECBE                  * Length of ECB extension        01030000
